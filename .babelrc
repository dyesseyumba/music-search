{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie >= 11"]
      },
      "loose": true
    }]
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "test": {
      "plugins": [
        ["istanbul", {
          "exclude": [
            "**/*.spec.js","./src/coverage"
          ]
        }]
      ]
    }
  }
}
