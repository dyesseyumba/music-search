  "author": "Dyesse YUMBA NDUBA <yumba.nduba@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js -s",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "cross-env NODE_ENV=production babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "test": "karma start",
    "test:watch": "karma start --silent --no-single-run"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-resource": "^1.6.4",
    "angular-ui-router": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "cheerio": "^1.0.0-rc.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-plugin-angular": "^2.4.2",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "fast-sass-loader": "^1.2.5",
    "file-loader": "^0.11.2",
    "fs": "^0.0.1-security",
    "fs-extra": "3.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "open": "^0.0.5",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-middleware-webpack-2": "^1.5.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-simple-progress-plugin": "^0.0.1"
  }
}
